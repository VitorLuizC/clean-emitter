{
  "name": "@bitty/event-emitter",
  "version": "0.0.0",
  "description": "Listen and emit events without dirty your classes, objects and functions with EventEmitter interface.",
  "cdn": "dist/event-emitter.js",
  "main": "dist/event-emitter.cjs.js",
  "unpkg": "dist/event-emitter.js",
  "types": "dist/event-emitter.d.ts",
  "module": "dist/event-emitter.es.js",
  "browser": "dist/event-emitter.js",
  "jsdelivr": "dist/event-emitter.js",
  "scripts": {
    "lint": "prettier --check \"./{src,examples}/**/*.ts\"",
    "lint:fix": "prettier --write \"./{src,examples}/**/*.ts\"",
    "test": "pnpm run lint && echo \"Error: no test specified\" && exit 1",
    "build:typescript": "tsc src/event-emitter -d --outDir dist --lib es2018 -t es2015",
    "build:bili": "bili dist/event-emitter --banner --formats cjs,es,umd --js buble",
    "build": "pnpm run build:typescript && pnpm run build:bili"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitorLuizC/event-emitter.git"
  },
  "keywords": [
    "event-emitter",
    "event",
    "emitter"
  ],
  "author": {
    "url": "https://vitorluizc.github.io/",
    "name": "Vitor Luiz Cavalcanti",
    "email": "vitorluizc@outlook.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VitorLuizC/event-emitter/issues"
  },
  "homepage": "https://github.com/VitorLuizC/event-emitter#readme",
  "devDependencies": {
    "bili": "^3.0.14",
    "prettier": "^2.1.2",
    "typescript": "^2.8.1"
  }
}
